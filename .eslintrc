{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jasmine": true
  },
  "parserOptions": {
    "allowImportExportEverywhere": false,
    "codeFrame": false,
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "plugins": [
    "react"
    # "jsx-a11y"
  ],
  "extends": [
    "plugin:react/recommended"
    # "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    # "for-direction": 0,
    # "no-await-in-loop": 0,
    "no-compare-neg-zero": 1,
    "no-cond-assign": [1,"except-parens"],
    # "no-console": 1,
    # "no-constant-condition": 0,
    "no-control-regex": 1,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 1,
    "no-empty-character-class": 1,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": [1,"all", {"ignoreJSX": "multi-line"}],
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-inner-declarations": [1,"both"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "no-obj-calls": 1,
    # "no-prototype-builtins": 0,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    # "no-template-curly-in-string": 0,
    "no-unexpected-multiline": 2,
    "no-unreachable": 1,
    # "no-unsafe-finally": 2,
    # "no-unsafe-negation": 0,
    "use-isnan": 1,
    "valid-jsdoc": [1, {
      "requireParamDescription": false,
      "requireReturnDescription": false,
      "requireReturn": false,
      "prefer": {"returns": "return"},
    }],

    # --------------
    # Best Practices
    # --------------

    "accessor-pairs": [1,{"getWithoutSet":false}],
    "array-callback-return": 0,
    "block-scoped-var": 1,
    # "complexity": [0,5],
    # "consistent-return": 0
    "curly": [1,"multi-line"],
    "default-case": 1,
    "dot-notation": [1,{"allowKeywords":true}],
    "dot-location": [1,"property"],
    "eqeqeq": [1,"smart"],
    "guard-for-in": 1,
    "no-alert": 1,
    "no-caller": 1,
    # "no-case-declarations": 2,
    "no-div-regex": 1,
    "no-else-return": 1,
    # "no-empty-function": 0,
    # "no-empty-pattern": 2,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    # "no-extra-label": 0,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    # "no-global-assign": 0,
    "no-implicit-coercion": [1,{"boolean":true,"number":true,"string":true}],
    # "no-implicit-globals": 0,
    "no-implied-eval": 1,
    # "no-invalid-this": 0,
    "no-iterator": 1,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-loop-func": 1,
    # "no-magic-numbers": 0,
    "no-multi-spaces": 1,
    "no-multi-str": 1,
    # "no-new": 0,
    # "no-new-func": 0,
    "no-new-wrappers": 1,
    # "no-octal": 2,
    # "no-octal-escape": 0,
    "no-param-reassign": [1,{"props":true}],
    "no-proto": 1,
    "no-redeclare": [1,{"builtinGlobals":false}],
    # "no-restricted-properties": 0,
    "no-return-assign": [1,"except-parens"],
    "no-script-url": 1,
    # "no-self-assign": 2,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 1,
    # "no-unmodified-loop-condition": 0,
    "no-unused-expressions": 1,
    # "no-unused-labels": 2,
    "no-useless-call": 1,
    # "no-useless-concat": 0,
    # "no-useless-escape": 0,
    "no-void": 1,
    "no-warning-comments": 0,
    "no-with": 1,
    # "prefer-promise-reject-errors": 0,
    "radix": 1,
    # "require-await": 0,
    "valid-typeof": 1,
    "vars-on-top": 1,
    "wrap-iife": [1,"outside"],
    "yoda": [1,"never",{"exceptRange":true,"onlyEquality":false}],

    # -----------
    # Strict Mode
    # -----------

    # "script": 0,
    "strict": [1,"never"],

    # ---------
    # Variables
    # ---------

    "init-declarations": 0,
    "no-catch-shadow": 1,
    "no-delete-var": 1,
    "no-label-var": 1,
    # "no-restricted-globals": 0,
    "no-shadow": [1,{"builtinGlobals":true,"hoist":"all"}],
    "no-shadow-restricted-names": 1,
    "no-undef": 1,
    "no-undef-init": 1,
    # "no-undefined": 0,
    "no-unused-vars": [1, {args: "none"}],
    "no-use-before-define": 1,

    # --------------------
    # Node.js and CommonJS
    # --------------------

    "callback-return": 1,
    # "global-require": 0,
    "handle-callback-err": [1,"^.*(e|E)rr"],
    # "no-buffer-constructor": 0,
    "no-mixed-requires": [1,true],
    "no-new-require": 1,
    "no-path-concat": 1,
    # "no-process-env": 0,
    # "no-process-exit": 0,
    "no-restricted-modules": 0,
    "no-sync": 1,

    # ----------------
    # Stylistic Issues
    # ----------------

    "array-bracket-newline": 0,
    "array-bracket-spacing": [1,"never",{"singleValue":false,"objectsInArrays":false,"arraysInArrays":true}],
    "array-element-newline": 0,
    "block-spacing": [1, "always"],
    "brace-style": [1,"1tbs",{"allowSingleLine":true}],
    "camelcase": [1,{"properties":"always"}],
    # "capitalized-comments": 0,
    "comma-dangle": [1,{
    "arrays": "only-multiline",
    "objects": "only-multiline",
    "imports": "never",
    "exports": "never",
    "functions": "never",
    }],
    "comma-spacing": [1,{"after":true}],
    "comma-style": 1,
    "computed-property-spacing": [1,"never"],
    # "consistent-this": [0,"_this"],
    "eol-last": 1,
    "func-call-spacing": 1,
    # "func-name-matching": 0,
    # "func-names": [0, "as-needed"],
    # "func-style": [0,"expression"],
    # "id-blacklist": 0,
    # "id-length": 0,
    # "id-match": 0,
    # "indent": 0,
    # "jsx-quotes": 0,
    "key-spacing": [1,{"afterColon":true}],
    "keyword-spacing": [1, {"before": true, "after": true, "overrides": {} }],
    # "line-comment-position": 0,
    # "linebreak-style": 0,
    # "lines-around-comment": [0,{"beforeBlockComment":true,"beforeLineComment":true,"allowBlockStart":true,"allowBlockEnd":true}],
    # "max-depth": 0,
    # "max-len": 0,
    # "max-lines": 0,
    "max-nested-callbacks": [1,3],
    # "max-params": 0,
    # "max-statements": 0,
    # "max-statements-per-line": 0,
    # "multiline-ternary": 0,
    "new-cap": [1,{"newIsCap":true,"capIsNew":true}],
    "new-parens": 1,
    # "newline-per-chained-call": 0,
    "no-array-constructor": 1,
    # "no-bitwise": 0,
    "no-continue": 1,
    # "no-inline-comments": 0,
    "no-lonely-if": 1,
    # "no-mixed-operators": 0,
    "no-mixed-spaces-and-tabs": [1,"smart-tabs"],
    # "no-multi-assign": 0,
    "no-multiple-empty-lines": [1,{"max":1}],
    # "no-negated-condition": 0,
    "no-nested-ternary": 1,
    "no-new-object": 1,
    # "no-plusplus": 0,
    # "no-restricted-syntax": 0,
    "no-tabs": 1,
    # "no-ternary": 0,
    "no-trailing-spaces": [1,{"skipBlankLines":true}],
    # "no-underscore-dangle": 0,
    "no-unneeded-ternary": 1,
    # "no-whitespace-before-property": 0,
    # "nonblock-statement-body-position": 0,
    # "object-curly-newline": 0,
    "object-curly-spacing": [1, "always"],
    # "object-property-newline": 0,
    "one-var": [1,"never"],
    # "one-var-declaration-per-line": 0,
    "operator-assignment": [1,"always"],
    "operator-linebreak": [1,"after"],
    "padded-blocks": [1,"never"],
    # "padding-line-between-statements": 0,
    "quote-props": [1,"consistent-as-needed"],
    "quotes": [1,"single","avoid-escape"],
    "semi": [1,"always"],
    "semi-spacing": [1,{"after":true}],
    # "semi-style": 0,
    # "sort-keys": 0,
    "sort-vars": [1,{"ignoreCase":true}],
    "space-before-blocks": [1,"always"],
    "space-before-function-paren": [1,{"anonymous":"never","named":"never","asyncArrow":"always"}],
    # "space-in-parens": [1,"never",{}],
    "space-infix-ops": [1,{"int32Hint":true}],
    "space-unary-ops": [1,{"words":false,"nonwords":false}],
    "spaced-comment": [1,"always",{}],
    # "switch-colon-spacing": 1, //TODO: Not working
    # "template-tag-spacing": 0,
    # "unicode-bom": 0,
    "wrap-regex": 1,

    # ------------
    # ECMAScript 6
    # ------------

    # "arrow-body-style": 0,
    "arrow-parens": [1, "as-needed"],
    "arrow-spacing": [1,{"before":true,"after":true}],
    "constructor-super": 1,
    "generator-star-spacing": [1,"after"],
    "no-class-assign": 1,
    # "no-confusing-arrow": 0,
    "no-const-assign": 1,
    "no-dupe-class-members": 1,
    "no-duplicate-imports": 1,
    "no-new-symbol": 1,
    # "no-restricted-imports": 0,
    "no-this-before-super": 1,
    # "no-useless-computed-key": 0,
    # "no-useless-constructor": 0,
    # "no-useless-rename": 0,
    "no-var": 1,
    "object-shorthand": [1,"always"],
    # "prefer-arrow-callback": 0,
    "prefer-const": 0,
    # "prefer-destructuring": 0,
    # "prefer-numeric-literals": 0,
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "prefer-reflect": 0,
    # "prefer-template": 0,
    "require-yield": 1,
    "rest-spread-spacing": 1,
    # "sort-imports": 0,
    # "symbol-description": 0,
    # "template-curly-spacing": 0,
    "yield-star-spacing": [1, "after"]
  }
}
