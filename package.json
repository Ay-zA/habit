{
  "name": "habit",
  "version": "1.0.0",
  "description": "I'll just remind you habits",
  "main": "index.js",
  "scripts": {
    "lint": "eslint app --ext .jsx,.js",
    "clean": "rimraf ./build",
    "build:watch": "npm run clean && cross-env NODE_ENV=development webpack -d --watch",
    "build:dev": "npm run clean && cross-env NODE_ENV=development webpack --config webpack.config.dev.babel.js -d",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.prod.babel.js -p",
    "server:dev": "cross-env NODE_ENV=dev node app/index.js --open",
    "server:node": "cross-env NODE_ENV=node webpack --watch",
    "server:prod": "cross-env NODE_ENV=prod node app/index.js",
    "start": "cross-env NODE_ENV=development && nodemon -q app/index.js --port 80",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/frontendmonster/habit.git"
  },
  "keywords": [
    "habit",
    "todo",
    "reminder",
    "cross-platform",
    "minimal"
  ],
  "author": "S-Kill <frontendmonster@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frontendmonster/habit/issues"
  },
  "engines": ">=8.9.0",
  "homepage": "https://github.com/frontendmonster/habit#readme",
