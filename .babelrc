{
  "presets": ["@babel/env", "@babel/flow", "@babel/stage-0", "@babel/preset-react"],
  "retainLines": true,
  "plugins": [
    "react-hot-loader/babel",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "~": "./",
          "@": "./app/server",
          "-": "./app/client"
        }
      }
    ]
  ]
}
