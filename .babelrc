{
  "presets": ["env", "flow", "stage-0", "react"],
  "retainLines": true,
  "plugins": [
    "react-hot-loader/babel",
    "babel-plugin-add-module-exports",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "~": "./",
          "@": "./app/server",
          "-": "./app/client"
        }
      }
    ]
  ]
}
